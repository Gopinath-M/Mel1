<%if !request.xhr?%>
  <div class="CenterNav">
    <h2 class="b-head"><%=t('labels.manage_facility_ict_service')%></h2>
  <%end%>
  <div id="div_ajax" >
    <%if @facility_ict_service && !@facility_ict_service.empty?%>
        <%@facility_ict_service.each do |ict_service|%>
          <div class="manage-user box">
            <div class="manage-user-row box">
              <div class="mu-c1">
              </div>
              <div class="mu-c2">
                <p class="head1"><%=ict_service.name%></p>
              </div>
              <div class="mu-c3">
                <%= link_to image_tag("../images/edit.jpg",:width=>"14",:height=>"14",:alt=>"img") +t("labels.edit"), :action=>'edit', :id=>ict_service.id.to_i%>
                <%if ict_service.is_active %>
                  <%= link_to image_tag("../images/deactive.jpg",:width=>"14",:height=>"14",:alt=>"img") +t("labels.deactivate"), update_status_facility_ict_services_path(:id=>ict_service.id, :status=>"Deactivate"), :confirm => t('labels.service_deact_confirm'), :method=>:post, :title=>t("labels.deactivate")%>
                <%else%>
                  <%= link_to  image_tag("../images/active.jpg",:width=>"14",:height=>"14",:alt=>"img") +t("labels.activate"),  update_status_facility_ict_services_path(:id=>ict_service.id,:status=>"Activate"), :confirm => t('labels.service_act_confirm'), :method=>:post, :title=>t("labels.activate")%>
                <%end%>

              </div>
            </div>

          </div>
        <%end%><div class="manage-user-rows box"><br/><br/>
<%#= paginate @agencies %></div>
    <%else%>
      <%=t('labels.no_data_available')%>
    <%end%>
<%#*<div><%= paginate @units %>
  </div>
</div>