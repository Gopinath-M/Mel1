<div class="clearfix"></div>
<div class="CenterNav">
  <div class="form-sec">
      
      <%= form_for(@newsletter_image, :html => { :method => :post, :multipart => true }) do |f| %>
        <div class="form-sec">
          <%=f.error_messages%>
          <div class="clearfix">&nbsp;</div>
          <div class="form-sec-row">
            <label class="text"><%=t('labels.image')%><span style=color:red>*</span></label>
            <span class="in-box">
               <%= f.file_field :image%>
            </span>
          </div>
          <div class="form-sec-row">
            <label class="text">&nbsp;</label>
            <span class="in-box">
              <%= f.submit :class=>"btn2", :value=>@newsletter_image.new_record? ? t('labels.create') : t('labels.update')%>
            </span>
          </div>
        </div>
      <%end%>
  </div>
</div>
