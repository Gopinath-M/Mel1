<%= form_for(@agency) do |f| %>
  <%=f.error_messages%>
  <div class="clearfix">&nbsp;</div>
  <div class="form-sec">
    <span class="in-box">
      <span class="ui-watermark-container ui-watermark-input">
        <label for="" class="ui-watermark-label" style="left: 6px; top: 6px;"></label>


        <div class="form-sec-row">
          <label class="text">Agency Name</label>
          <span class="in-box">
            <span class="ui-watermark-container ui-watermark-input">
              <label for="" class="ui-watermark-label" style="left: 6px; top: 6px;"></label>
              <%= f.text_field :name%>
            </span>
          </span>
        </div>

        <div class="form-sec-row">
          <label class="text">Is Active</label>
          <span class="in-box">
            <span class="ui-watermark-container ui-watermark-input">
              <label for="" class="ui-watermark-label" style="left: 6px; top: 6px;"></label>
              <%= f.check_box :is_active%>
            </span>
          </span>
        </div>

        <div class="form-sec-row">
          <label class="text">&nbsp;</label>
          <span class="in-box">
            <%= f.submit :class=>"btn2"%>
          </span>
        </div>
        </div>
      </span>
    </span>
  <%end%>
