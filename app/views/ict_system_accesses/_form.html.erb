<%= form_for(@ict_system_access, :html => { :method => :post, :multipart => true }) do |f| %>
  <%#=error_messages%>
  <div class="clearfix">&nbsp;</div>
  <%#*<div class="CenterNav">%>
  <h2 class="b-head">ICT - System Access</h2>
  <div class="form-sec">
  <%# if @manager.present? %>
   
    </div>
      <font color="#369"><br/><b><u>ICT Facility :-</u><br/><br/></b></font>

      <div class="form-sec-row">
        <label class="text">Type of Facility<span style=color:red>*</span></label>
        <span class="in-box">
          <%= f.select(:requisition_type_id, RequisitionType.all.collect {|p| [ p.name, p.id ] },:prompt => "SELECT A ICT FACILITY", :style=> "width:132%;") %>
        </span>
      </div>
      <div class="form-sec-row">
          <label class="text">Required System<span style=color:red>*</span></label>
          <span class="in-box">
            <%= f.select(:system_access_id, SystemAccess.all.collect {|p| [ p.name, p.id ] },:prompt => "SELECT A REQUIRED SYSTEM", :style=> "width:132%;") %>
          </span>
      </div>
          <div class="form-sec-row">
            <label class="text">Upload File<span style=color:red>*</span></label>
              <span class="in-box">
                <%= f.file_field :system_access_attachment%>
                <%= f.hidden_field :system_access_attachment_cache %>
              </span>
          </div>
         
          <div class="form-sec-row">
            <label class="text">Remarks<span style=color:red>*</span></label>
            <span class="in-box">
              <%= f.text_area :remarks, :rows =>4%></span>
            <div>

            </div>
          </div>
          <div class="form-sec-row">
            <div class="CenterNav">
              <label class="text">&nbsp;</label>
              <span class="in-box">
                <%= f.submit :value=>'Save', :class=>"btn2"%>
              </span>
            </div>
          </div>
  <%#*</div>%>
        <%end%>
